<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Configurator">

    <Button
        android:id="@+id/button_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/next"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="683dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="localhost"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="148dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        android:text="8080"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="228dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="noizalyzer"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="308dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter server"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="163dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter port"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="242dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter endpoint"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="322dp" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="411dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Write timeout (number)"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="395dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Write timeout (unit)"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="459dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mic read (number)"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="512dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mic read (unit)"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="571dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="58dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="number"
        android:text="1"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="389dp" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="108dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="text"
        android:text="MINUTES"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="447dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="59dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="number"
        android:text="3"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="500dp" />

    <EditText
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="118dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="text"
        android:text="SECONDS"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="557dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView,textView3,editTextTextPersonName3,editTextTextPersonName2,textView2,editTextTextPersonName" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView4,editTextTextPersonName4,editTextTextPersonName5,textView5" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView6,editTextTextPersonName7,textView7" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView,editTextTextPersonName,editTextTextPersonName2,textView2,textView3,editTextTextPersonName3" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView4,editTextTextPersonName4,editTextTextPersonName5,textView5" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="textView6,editTextTextPersonName6,editTextTextPersonName7,textView7" />
</androidx.constraintlayout.widget.ConstraintLayout>